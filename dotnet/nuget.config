<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="CCI-Dependency" value="https://msazure.pkgs.visualstudio.com/_packaging/CCI-Dependency/nuget/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="CCI-Dependency">
      <package pattern="Microsoft.Bot.*" />
      <package pattern="Microsoft.PowerFx.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
